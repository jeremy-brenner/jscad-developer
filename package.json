{
  "name": "jscad-developer",
  "version": "1.0.0",
  "description": "jscad developer",
  "main": "main.js",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeremy-brenner/jscad-developer.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jeremy-brenner/jscad-developer/issues"
  },
  "homepage": "https://github.com/jeremy-brenner/jscad-developer#readme",
  "build": {
    "appId": "net.drshoggoth.jscad-developer",
    "linux": {
      "category": "Utility",
      "target": [
        "snap"
      ]
    }
  },
  "dependencies": {
    "electron-root-path": "^1.0.16",
    "electron-settings": "^3.2.0",
    "node-watch": "^0.6.3"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "electron": "^8.2.3",
    "electron-builder": "^22.5.1",
    "rollup": "^1.20.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.1.2",
    "svelte": "^3.0.0",
    "svelte-routing": "^1.4.2"
  }
